class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        for(int i =0;i<nums.size();i++){
            cin>>nums[i];
        }
        for(int i=0;i<nums.size();i++){
            if(nums[i] == 0){
                nums.push_back(nums[i]);
            }
        }
        for(int i=0;i<nums.size();i++){
            cout<<nums[i];
        }
    }
};
